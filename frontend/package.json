{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "relay-compiler && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "relay": "relay-compiler",
    "relay:watch": "relay-compiler --watch",
    "fetch-schema": "get-graphql-schema http://localhost:8000/graphql > src/relay/schema.graphql",
    "dev:relay": "concurrently \"npm run dev\" \"npm run relay:watch\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.7",
    "fetch-event-source": "^1.0.0-alpha.2",
    "fetch-event-stream": "^0.1.5",
    "graphql-sse": "^2.5.4",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^16.2.0",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "relay-runtime": "^16.2.0",
    "vite-plugin-relay-lite": "^0.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@react-router/dev": "^7.2.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-relay": "^16.0.5",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "babel-plugin-relay": "^16.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "get-graphql-schema": "^2.1.2",
    "relay-compiler": "^16.2.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
